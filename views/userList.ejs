<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>User List</title>
</head>
<body>
<h1><%= animal.name %></h1>

<h1>Number Of Dogs</h1>
<h1><%= numberOfDogs %></h1>


<h1>User List</h1>
<ul>
    <% users.forEach(function(user) { %>
        <li><%= user.name %> - <%= user.age %></li>
    <% }); %>

</ul>

</body>
</html>